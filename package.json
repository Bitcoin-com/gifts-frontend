{
  "name": "bitcoincom-boilerplate",
  "private": true,
  "description": "Bitcoin.com Boilerplate",
  "version": "1.0.0",
  "author": "Bitcoin.com",
  "license": "MIT",
  "scripts": {
    "start": "yarn develop",
    "start:local": "gatsby develop",
    "compress-images": "node image-compressor.js",
    "build": "yarn compress-images && yarn gatsby clean && cross-env NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=production gatsby build",
    "build:sandbox": "yarn compress-images && yarn gatsby clean && cross-env NODE_OPTIONS=--max_old_space_size=4096 gatsby build",
    "serve": "gatsby serve",
    "move": "node move.js",
    "develop": "gatsby develop -H 0.0.0.0 -p 3000",
    "format": "prettier --write '**/*.js'",
    "link:sb": "yarn link bitcoincom-storybook",
    "link:un": "yarn link bitcoincom-universal",
    "link:all": "yarn link bitcoincom-storybook bitcoincom-universal",
    "unlink:sb": "yarn unlink bitcoincom-storybook",
    "unlink:un": "yarn unlink bitcoincom-universal",
    "unlink:all": "yarn unlink bitcoincom-storybook bitcoincom-universal",
    "upgrade:sb": "yarn upgrade bitcoincom-storybook",
    "upgrade:un": "yarn upgrade bitcoincom-universal",
    "upgrade:all": "yarn upgrade bitcoincom-storybook bitcoincom-universal",
    "lint": "eslint src gatsby-browser.js gatsby-config.js gatsby-node.js gatsby-ssr.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitcoin-portal/bitcoincom-boilerplate.git"
  },
  "babel": {
    "presets": [
      "babel-preset-gatsby"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-plugin-styled-components": "^1.10.0",
    "bitcoincom-storybook": "git+ssh://git@github.com/bitcoin-portal/bitcoincom-storybook.git",
    "bitcoincom-universal": "git+ssh://git@github.com/bitcoin-portal/bitcoincom-universal.git",
    "gatsby": "^2.7.1",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-netlify-cms": "^4.1.22",
    "gatsby-plugin-offline": "^2.1.3",
    "gatsby-plugin-preconnect": "^1.0.5",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sitemap": "^2.2.16",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "netlify-cms-app": "^2.9.7",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.8.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-intl": "^3.2.0",
    "babel-preset-gatsby": "^0.1.11",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "fs-extra": "^8.0.1",
    "gatsby-plugin-eslint": "^2.0.5",
    "gatsby-plugin-react-svg": "^2.1.1",
    "image-size": "^0.7.4",
    "prettier": "^1.18.0",
    "sharp": "^0.22.1"
  }
}
