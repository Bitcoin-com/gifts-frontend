{
  "name": "bitcoincom-boilerplate",
  "private": true,
  "description": "Bitcoin.com Boilerplate",
  "version": "1.0.0",
  "author": "Bitcoin.com",
  "license": "MIT",
  "scripts": {
    "start": "yarn develop",
    "start:local": "gatsby develop",
    "compress-images": "node image-compressor.js",
    "build": "yarn compress-images && yarn gatsby clean && cross-env NODE_ENV=production gatsby build --no-uglify",
    "build:sandbox": "yarn compress-images && yarn gatsby clean && cross-env gatsby build",
    "serve": "gatsby serve",
    "develop": "gatsby develop -H 0.0.0.0 -p 3000",
    "format": "prettier --write '**/*.js'",
    "link:sb": "yarn link bitcoincom-storybook",
    "link:un": "yarn link bitcoincom-universal",
    "link:all": "yarn link bitcoincom-storybook bitcoincom-universal",
    "unlink:sb": "yarn unlink bitcoincom-storybook",
    "unlink:un": "yarn unlink bitcoincom-universal",
    "unlink:all": "yarn unlink bitcoincom-storybook bitcoincom-universal",
    "upgrade:sb": "yarn upgrade bitcoincom-storybook",
    "upgrade:un": "yarn upgrade bitcoincom-universal",
    "upgrade:all": "yarn upgrade bitcoincom-storybook bitcoincom-universal",
    "lint": "eslint src gatsby-browser.js gatsby-config.js gatsby-node.js gatsby-ssr.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitcoin-portal/bitcoincom-boilerplate.git"
  },
  "babel": {
    "presets": [
      "babel-preset-gatsby"
    ]
  },
  "dependencies": {
    "@react-pdf/renderer": "^1.6.8",
    "@react-pdf/styled-components": "^1.4.0",
    "axios": "^0.19.0",
    "babel-plugin-styled-components": "^1.10.6",
    "badger-components-react": "^0.9.0",
    "bitbox-sdk": "^8.11.2",
    "bitcoincom-storybook": "git+ssh://git@github.com/bitcoin-portal/bitcoincom-storybook.git#v1.0.0",
    "bitcoincom-universal": "git+ssh://git@github.com/bitcoin-portal/bitcoincom-universal.git#v1.0.0",
    "brace": "^0.11.1",
    "gatsby": "^2.15.34",
    "gatsby-plugin-manifest": "^2.2.20",
    "gatsby-plugin-netlify-cms": "^4.1.25",
    "gatsby-plugin-offline": "^3.0.13",
    "gatsby-plugin-preconnect": "^1.0.5",
    "gatsby-plugin-react-helmet": "^3.1.11",
    "gatsby-plugin-sitemap": "^2.2.16",
    "gatsby-plugin-styled-components": "^3.1.8",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "jsoneditor": "^7.1.0",
    "jsoneditor-react": "^1.0.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "netlify-cms-app": "^2.9.7",
    "prop-types": "^15.7.2",
    "qrcode": "^1.4.4",
    "react": "^16.10.2",
    "react-copy-to-clipboard": "^5.0.2",
    "react-datepicker": "^2.12.1",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-intl": "^3.3.2",
    "react-qrcode-logo": "^2.2.1",
    "react-spring": "^8.0.27",
    "reactjs-popup": "^1.5.0",
    "styled-components": "^4.4.0",
    "toasted-notes": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-intl": "^4.1.20",
    "babel-preset-gatsby": "^0.2.17",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "fs-extra": "^8.0.1",
    "gatsby-plugin-eslint": "^2.0.5",
    "gatsby-plugin-react-svg": "^2.1.1",
    "image-size": "^0.7.4",
    "prettier": "^1.18.0",
    "sharp": "^0.22.1"
  }
}
